<%- include("index"); -%>
<div class="card" style="width: 70%; margin-top: 50px;">
  <div class="card-body">
    <h3 class="card-title"><%=FirstName%>  <%=LastName%></h3>
    <h6 class="card-subtitle mb-2 text-muted" style="padding-bottom: 20px;"><%=Team%></h6>
    <p class="card-text" style="margin-bottom: 5px;"> <b>Height:&nbsp;</b><%=HeightFeet%>Ft <%=HeightInches%>in &nbsp;&nbsp;&nbsp;&nbsp; <b>Weight:</b>&nbsp;<%=Weight%>&nbsp;Pounds</p>
    <p class="card-text"> <b>Position:</b> <%=Position%></p>
  </div>
</div>
